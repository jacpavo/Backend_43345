
<div>   
    <h1>Productos:</h1>
    {{#each products.docs}}
    <div>
        <h4>{{this.title}} &nbsp; </h4>
        <p>Descripcion: {{this.description}} &nbsp; </p>
        <p>Precio: ${{this.price}} &nbsp; </p>
        <p>Categoria: {{this.category}} &nbsp; </p>
        <img src="{{this.thumbnail}}" alt="{{this.description}}">
        <p>Stock: {{this.stock}} &nbsp; </p>
        <button onclick="addToCartButton()">AÃ±adir al carrito</button>
        <hr>
    </div>
    <br/>
    {{/each}}
</div>
<br/>
<div>
    {{#if products.hasPrevPage}}
        <a href={{products.prevLink}}><< Anterior</a>
    {{/if}}
    &nbsp;
    &nbsp;
    {{#if products.hasNextPage}}
        <a href={{products.nextLink}}>Siguiente >></a>
    {{/if}} 
</div>
